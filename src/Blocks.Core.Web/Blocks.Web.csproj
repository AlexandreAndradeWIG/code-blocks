<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>0.0.1</Version>
    <Authors>WIG</Authors>
    <PackageId>blocks.web</PackageId>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageProjectUrl>https://github.com/WorkIsGood/blocks</PackageProjectUrl>
    <PackageIcon>blocks_logo.png</PackageIcon>
    <RepositoryUrl>https://github.com/WorkIsGood/wig-core</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
